<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.agroapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/splash_image"
        android:label="@string/app_name"
        android:roundIcon="@drawable/splash_image"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProfileActivity"></activity>
        <activity android:name=".RequestListActivity" />
        <activity android:name=".NonSupplierTradeActivity" />
        <activity android:name=".RequestCropActivity" />
        <activity android:name=".NonSupplierActivity" />
        <activity android:name=".FarmerTradeActivity" />
        <activity android:name=".SupplierTradeActivity" />
        <activity android:name=".SellHistoryActivity" />
        <activity android:name=".BuyCropActivity" />
        <activity android:name=".SellCropActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SupplierActivity" />
        <activity android:name=".FarmerActivity" />
        <activity android:name=".RegisterActivity" />
        <activity
            android:name=".MainActivity"
            android:label="Home" />
    </application>

</manifest>